package Try;
import java.util.*;

public class Array {
	private ArrayList<String> Adress;
	
	public Array(){
		Adress = new ArrayList<String>();
	}
	public void voegtoe( String status){
		Adress.add(status);
	}
	public void show (){
		for (String status : Adress ){
			System.out.println(status);
		}
	}
}
