
public class tijdstip {
	private int uren;
	private int min; 
	private int sec;
	
	public tijdstip(int uren, int min, int sec) {
		getUren(uren);
		getMin(min);
		getSec(sec);
	}
	public int getUren() {
		return uren;
	}
	public void setUren(int uren) {
		this.uren = uren;
	}
	public int getMin() {
		return min;
	}
	public void setMin(int min) {
		this.min = min;
	}
	public int getSec() {
		return sec;
	}
	public void setSec(int sec) {
		this.sec = sec;
	} 
	
}
